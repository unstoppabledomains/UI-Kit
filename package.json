{
  "name": "@unstoppabledomains/ui-kit",
  "version": "1.0.0",
  "description": "A set of common Unstoppable Domains components",
  "homepage": "https://github.com/unstoppabledomains/UI-Kit#readme",
  "bugs": {
    "url": "https://github.com/unstoppabledomains/UI-Kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unstoppabledomains/UI-Kit"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:modules && yarn build:types && yarn build-storybook",
    "build:modules": "babel src --out-dir dist --extensions '.ts' --presets @babel/preset-typescript --plugins @babel/plugin-transform-runtime",
    "build:types": "tsc",
    "build-storybook": "build-storybook",
    "eslint:check": "eslint --ext .ts, .tsx, .js, .jsx, .json, .md .",
    "eslint:fix": "eslint --ext .ts, .tsx, .js, .jsx, .md, .json --fix .",
    "prettier:check": "prettier --check --config .prettierrc --ignore-path .prettierignore \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:fix": "prettier --write -l --config .prettierrc --ignore-path .prettierignore \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^17.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "tss-react": "^4.0.0-beta.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "engines": {
    "node": "16.x.x",
    "yarn": "1.22.x"
  },
  "publicConfig": {
    "access": "public"
  }
}
